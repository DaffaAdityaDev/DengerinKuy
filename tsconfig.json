{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/api/postMusic.js", "src/pages/api/getMusic.js", "src/pages/api/getallmusic.js", "src/pages/api/testfunction.js", "src/pages/api/Model/Music.js", "src/pages/api/Model/Genre.js", "src/pages/api/Model/Artist.js", "src/pages/api/Model/Album.js", "src/pages/api/Middleware/db.js"],
  "exclude": ["node_modules"]
}
